      <owl:Restriction>
        <owl:onProperty rdf:resource="https://saref.etsi.org/core/hasTime"/>
        <owl:onClass rdf:resource="https://saref.etsi.org/saref4ener/DurationUncertainty"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"
        >1</owl:maxQualifiedCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
